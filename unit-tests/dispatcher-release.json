{
    "actions": [
        {
            "command": "deploy_linaro_image",
            "parameters": {
                "image": "http://images.validation.linaro.org/kvm/large-jessie.img.gz"
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "http://git.linaro.org/git/lava-team/lava-functional-tests.git",
                        "testdef": "unit-tests/lava/dispatcher-release-unit-tests.yaml"
                    }
                ]
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "http://validation.linaro.org/RPC2",
                "stream": "/public/personal/lava-auto/lava-daily/"
            }
        }
    ],
    "device_type": "kvm",
    "job_name": "dispatcher-master-unit-tests",
    "logging_level": "DEBUG",
    "timeout": 1800
}
